#import "NSString+Category.h"

@implementation NSString (Category)

+(NSString *)convertNull:(id)object
{
    if ([object isEqual:[NSNull null]]) {
        return @"";
    }
    else if ([object isKindOfClass:[NSNull class]])
    {
        return @"";
    }
    else if (object==nil){
        return @"";
    }
    return object;
}

@end
